{
  "expo": {
    "name": "React native demo ",
    "description": "React native demo ",
    "slug": "react-native-demo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "gianlucatappata.react-native-demo",
      "buildNumber": "1.1.0"
    },
    "android": {
      "versionCode": 1010,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "splash": {
        "backgroundColor": "#ffffff",
        "image": "./assets/splash.png"
      },
      "package": "gianlucatappata.reactnativedemo"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "assets/icon.png",
          "color": "#ff0000",
          "sounds": [
            "assets/notifications.wav"
          ],
          "mode": "development"
        }
      ]
    ],
    "androidStatusBar": {
      "backgroundColor": "#fff",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "4f740709-502b-46a9-bc08-7f96022287a4"
      }
    }
  }
}
